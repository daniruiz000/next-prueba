generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("NEXT_DATABASE_URL")
}

model User {
  id             Int      @id @default(autoincrement())
  createdAt      DateTime @default(now())
  nombre         String
  apellido       String
  segundo_apellido String
  telefono       String @unique
  email          String  @unique
  foto           Bytes?
  dni            String? @unique
  nie            String? @unique
}
